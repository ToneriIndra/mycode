<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.zhao.bos.vo.Decidedzone" table="bc_decidedzone" >
        <id name="id" >
            <generator class="assigned" />
        </id>
        <many-to-one lazy="false" name="staff" class="cn.zhao.bos.vo.Staff">
            <column name="staff_id" />
        </many-to-one>
        <property name="name" />
    </class>
</hibernate-mapping>
